##{pageId}-table tr td
    vertical-align: middle

##{pageId}-table.active .data-row:not(.active)
    display: none

##{pageId}-table.executed .data-row:not(.executed)
    display: none


##{pageId}-table .data-row .controls a
    text-decoration: none

##{pageId}-table .data-row .controls .control
    cursor: pointer
    opacity: 0
    color: #ef7f19

##{pageId}-table .data-row:hover .controls .control
    opacity: 0.5

##{pageId}-table .data-row:hover .controls .control:hover
    opacity: 1