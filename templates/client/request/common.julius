
$(document).ready(() => {
    const cancelButtons = $('.data-row .controls .request-cancel-button')
    cancelButtons.click(e => {
        if (confirm(#{messageRender MsgDepositConfirmCancellationText})) {
            $(e.currentTarget)
                .parents('td.controls')
                .find('form.request-cancel-form')
                .submit()
        }
    })
})
